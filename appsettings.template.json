{
    "Database": {
        "ConnectionString": "Server=HOST; Database=DATABASE; Username=USER; Password=PASSWORD"
    },
    "Cookie": {
        "JwtToken": "token",
        "Domain": ".example.com",
        "Expiration": 86400
    },
    "Jwt": {
        "SecurityKey": "SECRET",
        "EncryptionAlgorithm": "HS512",
        "Expiration": 86400,
